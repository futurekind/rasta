{
    "css": {
        "scss": {
            "src": "src/scss/**/*.scss"
        },
        "main": {
            "src": "src/scss/main.scss",
            "dist": "dist/css/main.css"
        },
        "rasta": {
            "src": "src/scss/rasta/_rasta.scss",
            "dist": "scss/rasta/_rasta.scss"
        }
    },

    "html": {
        "all": {
            "src": "src/html/**/*.hbs"
        },
        "main": {
            "src": "src/html/*.hbs"
        },
        "layout": {
            "src": "src/html/layout/default.hbs"
        },
        "includes": {
            "src": "src/html/includes/*.hbs"
        },
        "helper": {
            "src": "src/html/helper/*.js"
        },
        "data": {
            "src": "src/html/data/*.json"
        }
    }
}
